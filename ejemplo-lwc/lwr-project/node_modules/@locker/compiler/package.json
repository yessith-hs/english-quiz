    "description": "Locker secure module compiler",
    "main": "dist/index.cjs.js",
    "module": "dist/index.js",
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "rollup --config .rolluprc.cjs",
        "build:dev": "cross-env NODE_ENV=development yarn build",
