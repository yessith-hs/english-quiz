        "test": "yarn test:dev && yarn test:base"
    },
    "devDependencies": {
        "@locker/ast-lib-maker": "0.16.27",
        "@locker/shared": "0.16.27",
        "acorn": "8.7.1",
        "acorn-walk": "8.2.0",
        "magic-string": "0.26.1",
        "merge-options": "3.0.4",
        "normalize-path": "3.0.0"
    },
    "files": [
        "dist/",
        "docs/"
    ],
